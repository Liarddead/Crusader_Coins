<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbarSize="100dp"
    tools:context=".ui.Coins.CoinsFragment">

    <TextView
        android:id="@+id/textAntioch3"
        android:layout_width="134dp"
        android:layout_height="28dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/medievalsharp"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="Antioch"
        android:textSize="24sp"
        android:visibility="visible"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/Tripoli_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.199"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Antioch_button"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/Jerusalim_button"
        android:layout_width="134dp"
        android:layout_height="166dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:background="@null"
        android:padding="0px"
        android:scaleType="fitCenter"
        android:src="@drawable/kingdom_of_jerusalem"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.461"
        app:layout_constraintStart_toEndOf="@+id/Antioch_button"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.238"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/teutonic_button"
        android:layout_width="134dp"
        android:layout_height="166dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:background="@null"
        android:padding="0px"
        android:scaleType="fitCenter"
        android:src="@drawable/teutonic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toEndOf="@+id/Tripoli_button"
        app:layout_constraintTop_toBottomOf="@+id/Jerusalim_button"
        app:layout_constraintVertical_bias="0.411"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/Antioch_button"
        android:layout_width="134dp"
        android:layout_height="166dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:background="@null"
        android:onClick="Change"
        android:padding="0px"
        android:scaleType="fitCenter"
        android:src="@drawable/antioche"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.234"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/Tripoli_button"
        android:layout_width="134dp"
        android:layout_height="166dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:background="@null"
        android:padding="0px"
        android:scaleType="fitCenter"
        android:src="@drawable/tripoli"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.204"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Antioch_button"
        app:layout_constraintVertical_bias="0.409"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textAntioch"
        android:layout_width="134dp"
        android:layout_height="28dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/medievalsharp"
        android:includeFontPadding="false"
        android:text="Antioch"
        android:textSize="24sp"
        android:visibility="visible"
        android:gravity="center"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/Tripoli_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.199"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Antioch_button"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textJerusalim"
        android:layout_width="134dp"
        android:layout_height="28dp"
        android:layout_marginTop="8dp"
        android:fontFamily="@font/medievalsharp"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="Jerusalem"
        android:textSize="24sp"
        android:visibility="visible"
        app:autoSizeTextType="none"
        app:layout_constraintBottom_toTopOf="@+id/Tripoli_button"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.833"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Antioch_button"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textTripoli"
        android:layout_width="134dp"
        android:layout_height="28dp"
        android:fontFamily="@font/medievalsharp"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="Tripoli"
        android:textSize="24sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.205"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Tripoli_button"
        app:layout_constraintVertical_bias="0.093" />

    <TextView
        android:id="@+id/textTeutonic"
        android:layout_width="134dp"
        android:layout_height="28dp"
        android:fontFamily="@font/medievalsharp"
        android:gravity="center"
        android:includeFontPadding="false"
        android:text="Teutonic"
        android:textSize="24sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.833"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Tripoli_button"
        app:layout_constraintVertical_bias="0.093" />

</androidx.constraintlayout.widget.ConstraintLayout>